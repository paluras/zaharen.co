{
  "routes": [
    { "handle": "filesystem" },
    { "src": "/.*", "dest": "/index.html" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Content-Security-Policy",
          "value": "connect-src 'self' https://my-project-n0jf.onrender.com; object-src 'none'; base-uri 'self'; form-action 'self';"
        }
      ]
    }
  ]
}
